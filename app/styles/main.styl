// sprite.styl is auto generated by gulp and watch
@import 'lib/sprite'

// Custom styles here
h1,
h2,
h3,
h4 
  font-family 'Maven Pro', sans-serif
  font-weight 100
  color #555
  padding 0px

body 
  color #aaa
  font-size 1.8em
  font-weight 300
  font-family source-sans-pro, "Helvetica Neue", Helvetica, Arial, sans-serif, sans-serif

.icon-control 
  sprite($control)

.icon-world 
  sprite($world)

.icon
  display inline-block